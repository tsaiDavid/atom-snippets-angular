'.source.js':
  'angular-ui-router':
    'prefix': 'a-router'
    'body': '(function() {\n  \'use strict\';\n\n\n  angular.module(\'${1:app}\')\n  .config(config);\n\n  config.\$inject = [\'\\$stateProvider\', \'\\$urlRouterProvider\'];\n\n  function config(\\$stateProvider, \\$urlRouterProvider) {\n    \\$urlRouterProvider.otherwise(\'/${2}\');\n\n    \\$stateProvider\n      .state(\'${2}\', {\n        url: \'/${2}\',\n        templateUrl: \'${2}/${2}.html\',\n        controller: \'${2/(.+)/\\u\\1/g}Ctrl\',\n        controllerAs: \'${2}\',\n        data: {\n          title: \'${2/(.+)/\\u\\1/g}\'\n        }\n      })\n      .state(\'${3}\', {\n        url: \'/${3}\',\n        templateUrl: \'${3}/${3}.html\',\n        controller: \'${3/(.+)/\\u\\1/g}Ctrl\',\n        controllerAs: \'${3}\',\n        data: {\n          title: \'${3/(.+)/\\u\\1/g}\'\n        }\n      })\n      .state(\'${4}\', {\n        url: \'/${4}\',\n        templateUrl: \'${4}/${4}.html\',\n        controller: \'${4/(.+)/\\u\\1/g}Ctrl\',\n        controllerAs: \'${4}\',\n        data: {\n          title: \'${4/(.+)/\\u\\1/g}\'\n        }\n      })\n      .state(\'${5}\', {\n        url: \'/${5}\',\n        templateUrl: \'${5}/${5}.html\',\n        controller: \'${5/(.+)/\\u\\1/g}Ctrl\',\n        controllerAs: \'${5}\',\n        data: {\n          title: \'${5/(.+)/\\u\\1/g}\'\n        }\n      })\n      .state(\'${6}\', {\n        url: \'/${6}\',\n        templateUrl: \'${6}/${6}.html\',\n        controller: \'${6/(.+)/\\u\\1/g}Ctrl\',\n        controllerAs: \'${6}\',\n        data: {\n          title: \'${6/(.+)/\\u\\1/g}\'\n        }\n      });\n\n  }\n})();'